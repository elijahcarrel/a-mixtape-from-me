{
  "name": "nextjs-fastapi",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "setup-backend-venv": "python3 -m venv venv && . venv/bin/activate",
    "setup-backend-packages": "venv/bin/pip install -r requirements.txt && venv/bin/pip install -e .",
    "setup-backend": "npm run setup-backend-venv && npm run setup-backend-packages",
    "setup-frontend": "npm install",
    "setup": "npm run setup-backend && npm run setup-frontend",
    "backend-dev": "python3 -m uvicorn api.index:app --reload",
    "frontend-dev": "next dev",
    "dev": "concurrently \"npm run frontend-dev\" \"npm run backend-dev\"",
    "build": "next build",
    "start": "next start",
    "frontend-lint": "next lint",
    "frontend-typecheck": "tsc --noEmit",
    "frontend-test": "jest",
    "backend-lint": "venv/bin/ruff check backend/",
    "backend-lint-fix": "venv/bin/ruff check --fix backend/",
    "backend-typecheck": "venv/bin/mypy backend/",
    "backend-test": "venv/bin/pytest",
    "gen-db-schema-from-sqlmodels": "python3 scripts/generate_schema.py",
    "gen-openapi-from-fastapi": "python3 scripts/generate_openapi_spec.py",
    "gen-ts-from-openapi": "openapi-ts",
    "gen-all": "npm run gen-db-schema-from-sqlmodels && npm run gen-openapi-from-fastapi && npm run gen-ts-from-openapi"
  },
  "dependencies": {
    "@stackframe/stack": "^2.8.27",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "24.2.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "autoprefixer": "10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "9.32.0",
    "eslint-config-next": "15.4.5",
    "formik": "^2.4.6",
    "js-cookie": "^3.0.5",
    "next": "^15.4.5",
    "postcss": "^8.5.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "sass": "^1.89.2",
    "tailwindcss": "^4.1.11",
    "typescript": "5.9.2",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@hey-api/openapi-ts": "0.80.2",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "babel-jest": "^30.0.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "ts-jest": "^29.4.1"
  }
}
